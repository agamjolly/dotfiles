{
	"name": "live-server-preview",
	"displayName": "Live Server Preview",
	"description": "Preview your HTML file with localhost server live-reloading enabled",
	"version": "0.1.4",
	"publisher": "negokaz",
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"html",
		"livereload",
		"preview"
	],
	"galleryBanner": {
		"color": "#1A237E",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/negokaz/vscode-live-server-preview"
	},
	"bugs": {
		"url": "https://github.com/negokaz/vscode-live-server-preview/issues"
	},
	"license": "MIT",
	"icon": "doc/img/icon.png",
	"activationEvents": [
		"onCommand:extension.liveServerPreview.open"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.liveServerPreview.open",
				"title": "Show Live Server Preview"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42",
		"typescript": "^2.9.2",
		"vsce": "^1.54.0",
		"vscode": "^1.1.26"
	},
	"dependencies": {
		"live-server": "^1.2.1",
		"path": "^0.12.7"
	},
	"__metadata": {
		"id": "568a8977-8469-4f89-b30d-8d5e3c7a0ce7",
		"publisherId": "9de4ddc4-7edb-4edd-925c-e1873d81b415",
		"publisherDisplayName": "negokaz"
	}
}